{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "template-simu01": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/template-simu01",
      "sourceRoot": "projects/template-simu01/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/template-simu01",
            "index": "projects/template-simu01/src/index.html",
            "browser": "projects/template-simu01/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/template-simu01/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [                              
              "projects/template-simu01/src/assets",
                {
                  "glob": "**/*",                  
                  "input": "projects/template-simu01/public"               
                }                
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "projects/template-simu01/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "template-simu01:build:production"
            },
            "development": {
              "buildTarget": "template-simu01:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/template-simu01/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/assets",
              {
                "glob": "**/*",
                "input": "projects/template-simu01/public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "projects/template-simu01/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "951a6929-464d-4804-b6bb-bd6ced90303b"
  }
}
